{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "migrate:gen": "pnpm typeorm migration:generate -d ./src/database/data-source.ts ./src/database/migrations/Auto",
    "migrate:run": "pnpm typeorm migration:run -d ./src/database/data-source.ts",
    "migrate:revert": "pnpm typeorm migration:revert -d ./src/database/data-source.ts",
    "seed": "tsx src/database/seed.ts",
    "test": "vitest --config vitest.config.ts",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "@types/supertest": "^6.0.3",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "typeorm": "^0.3.26",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
